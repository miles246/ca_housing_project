    "from sklearn.svm import SVR\n",
    "\n",
    "def train_svr(X_train, y_train, kernel='rbf', C=1.0, epsilon=0.1):\n",
    "    model = SVR(kernel=kernel, C=C, epsilon=epsilon)\n",
    "    model.fit(X_train, y_train)\n",
    "    return model"
